<div class="col-lg-3">
  <%= render "sidebars/left_sidebar" %>
</div>

<div class="col-lg-6">
<% present @service, ServiceCardPresenter do |service_presenter| %>
  <%= service_presenter.print_name %>
  <%= service_presenter.print_description %>
  <%= service_presenter.print_price %>
  <%= service_presenter.print_rating %>
  <%= service_presenter.print_total_votes %>
  <%= service_presenter.buttons %>
<% end %>
</div>

<div class="col-lg-3">
  <%= render "sidebars/right_sidebar" %>
</div>

